<body>
    <nav class="navbar navbar-expand-sm bg-light navbar-primary">
        <!-- Brand/logo -->
        <a class="navbar-brand" href="#">
            <img src="bird.jpg" alt="logo" style="width:40px;">
        </a>

        <!-- Links -->
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" href="/">Acceuil</a>
            </li>
            <li class="nav-item">
